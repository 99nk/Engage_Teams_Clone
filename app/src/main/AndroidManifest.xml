<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.niharika.engage_ms_teams">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Engage_MS_Teams">
        <activity android:name=".authentication.ResetPassword"></activity>
        <activity android:name=".FeedbackActivity" />
        <activity android:name=".TeamsActivity" />
        <activity android:name=".MainChatActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".appIntro.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".appIntro.SplashScreen" />
        <activity android:name=".authentication.RegisterActivity" />
        <activity android:name=".authentication.LoginActivity" />
        <activity android:name=".GroupChatActivity" />
        <activity
            android:name=".utils.startInstantMeet"
            android:theme="@style/customTheme"></activity>
        <activity android:name=".utils.ScheduleMeet" />
        <activity android:name=".ChatActivity" />
        <activity android:name=".FindFriendsActivity" />
        <activity android:name=".ProfileActivity" android:theme="@style/customTheme"/>
        <activity android:name=".SettingsActivity" />
    </application>

</manifest>